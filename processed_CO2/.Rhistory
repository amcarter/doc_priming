deltoAF<- function (del) {
Rst<-0.0112372
R<-(del/1000+1)*Rst
AF<-R/(1+R)
AF
}
AFtodel <- function (AF) {
Rst<-0.0112372
R<- AF/(1-AF)
del<- (R/Rst-1)*1000
del
}
